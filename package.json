{
	"name": "@alpfilho/emana-web-toolkit",
	"version": "2.0.2",
	"description": "Componentes comuns para projetos Emana",
	"repository": "https://github.com/alpfilho/emana-web-toolkit",
	"license": "MIT",
	"author": {
		"name": "Alfredo Peres",
		"email": "alfredo@emana.digital"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"build": "cross-env NODE_ENV=production webpack",
		"build:all": "concurrently -n build,build-types 'yarn run build' 'yarn run build:types'",
		"build:types": "cross-env NODE_ENV=production ttsc --emitDeclarationOnly --noEmit false",
		"dev": "rm -rf dist && yarn run watch:all",
		"watch:all": "concurrently -n watch-build,watch-types 'yarn run watch:build' 'yarn run watch:build:types'",
		"watch:build": "cross-env NODE_ENV=development webpack --watch",
		"watch:build:types": "cross-env NODE_ENV=development ttsc --emitDeclarationOnly --noEmit false --watch"
	},
	"dependencies": {
    "framer-motion": "^1.8.4",
    "popmotion": "9.0.0-beta-8",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-is": "^16.12.0",
		"react-router-dom": "^5.1.2",
		"styled-components": "^5.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.8.4",
		"@babel/preset-env": "^7.8.4",
		"@babel/preset-react": "^7.8.3",
		"@babel/preset-typescript": "^7.8.3",
		"@types/node": "^13.7.6",
		"@types/react": "^16.9.23",
		"@types/react-dom": "^16.9.5",
		"@types/react-is": "^16.7.1",
		"@types/react-router-dom": "^5.1.3",
		"@types/styled-components": "^5.0.1",
		"@types/webpack": "^4.41.6",
		"@typescript-eslint/eslint-plugin": "^2.21.0",
		"@typescript-eslint/parser": "^2.21.0",
		"@zerollup/ts-transform-paths": "^1.7.11",
		"babel-loader": "^8.0.6",
		"babel-plugin-styled-components": "^1.10.7",
		"concurrently": "^5.1.0",
		"cross-env": "^7.0.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.18.3",
		"eslint-plugin-react-hooks": "^2.4.0",
		"prettier": "^1.19.1",
		"source-map-loader": "^0.2.4",
		"ts-node": "^8.6.2",
		"tsconfig-paths-webpack-plugin": "^3.2.0",
		"ttypescript": "^1.5.10",
		"typescript": "^3.8.2",
		"webpack": "^4.41.6",
		"webpack-cli": "^3.3.11"
	},
	"peerDependencies": {
		"framer-motion": "*",
		"react": "*",
		"react-dom": "*",
		"react-is": "*",
		"react-router-dom": "*",
		"styled-components": "*"
	},
	"publishConfig": {
		"access": "restricted",
		"registry": "https://npm.pkg.github.com"
	},
	"languageName": "node"
}
