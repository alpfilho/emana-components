		"dist",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"build": "rollup --config --environment BUILD:production",
		"build:dev": "rollup --config --environment BUILD:development",
		"watch:build": "rollup --config --watch --environment BUILD:development",
		"watch:yalc": "concurrently -n build,push-yalc 'yarn run watch:build' 'yarn run watch:yalc:push'",
		"watch:yalc:push": "nodemon --watch dist --exec 'yalc push --changed'"
	},
